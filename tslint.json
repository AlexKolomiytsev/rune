{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-airbnb"
  ],
  "jsRules": {},
  "rules": {
    "no-boolean-literal-compare": true,
    "import-name": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "quotemark": [true, "single"],
    "object-shorthand-properties-first": false,
    "no-console": {
      "severity": "warning"
    }
  },
  "rulesDirectory": []
}
